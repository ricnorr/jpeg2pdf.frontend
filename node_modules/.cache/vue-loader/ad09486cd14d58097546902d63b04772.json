/components/header/InfoHeader\";\nexport default {\n  name: \"UploadForm\",\n  components: {InfoHeader},\n  data: function () {\n    return {\n      files: \"\",\n      readyForSubmit: false,\n      error: null\n    }\n  },\n  methods: {\n    submitFile: function () {\n      let formData = new FormData();\n      for (let x of this.files) {\n        formData.append(\"file\", x);\n      }\n      console.log(formData);\n      this.$root.$emit(\"sendFiles\", formData);\n    },\n    handleFileSubmit: function (event) {\n      this.files = []\n      if (event.target.files.length > 10 || event.target.files.length === 0) {\n        this.error = \"Attached files number should be between 1 and 10\"\n        return;\n      }\n      for (let file of event.target.files) {\n        this.files.push(file);\n      }\n      this.readyForSubmit = true;\n      this.error = null;\n    }\n  }\n}\n</script>\n\n<style scoped>\n.form-box {\n  border: 1px solid darkgreen;\n}\n\n.form-box .file-choose {\n  margin-top: 1rem;\n  margin-bottom: 1rem;\n}\n\n.form-box .file-submit {\n  margin-top: 1rem;\n  margin-bottom: 1rem;\n}\n\n.file-download {\n  margin-top: 1rem;\n}\n\n.form-box .error {\n  color:red;\n  font-size: 0.8rem;\n}\n</style>"]}]}