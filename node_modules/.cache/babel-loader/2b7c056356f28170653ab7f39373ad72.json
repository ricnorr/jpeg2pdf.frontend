  beforeCreate() {\n    this.$root.$on(\"PostFile\", (formData) => {\n      this.sendSingleFile(formData);\n    });\n    this.$root.$on(\"downloadFile\", () => {\n      this.downloadFile();\n    });\n  }\n}\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"],"sourceRoot":"src"}]}